{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": ".nuxt/dist",
    "ignore": [
      "**/server/**"
    ],
    "predeploy": [
      "cp -rf static/ .nuxt/dist"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "renderer",
          "region": "asia-northeast1"
        }
      }
    ]
  }
}
