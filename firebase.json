{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": ".nuxt/dist",
    "ignore": [
      "**/server/**"
    ],
    "predeploy": [
      "rsync -ac static/ .nuxt/dist/"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "renderer",
          "region": "asia-northeast1"
        }
      }
    ]
  }
}
